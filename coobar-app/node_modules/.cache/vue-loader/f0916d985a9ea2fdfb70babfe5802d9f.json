l-col :span=\"10\">\n                                        <div class=\"grid-content bg-purple\">\n                                            <span class=\"item item2\" :class=\"item1Color\">\n                                                <i :class=\"item1Icon\"></i>\n                                                {{msg1}}\n                                            </span>\n                                        </div>\n                                    </el-col>\n                           </el-row> \n                            <el-row :gutter=\"20\">\n                                    <el-col :span=\"6\">\n                                        <div class=\"grid-content bg-purple\">\n                                            <span class=\"item item1\">手机号码</span>\n                                        </div>\n                                        </el-col>\n                                    <el-col :span=\"8\">\n                                        <div class=\"grid-content bg-purple\">\n                                            <el-input\n                                                placeholder=\"请输入手机号码\"\n                                                v-model=\"phone\" @focus=\"phoneFocus\" @blur=\"phoneBlur\"\n                                                clearable>\n                                            </el-input>\n                                        </div>\n                                    </el-col>\n                                    <el-col :span=\"10\">\n                                        <div class=\"grid-content bg-purple\">\n                                            <span class=\"item item2\" :class=\"item2Color\">\n                                                <i :class=\"item2Icon\"></i>\n                                                {{msg2}}\n                                            </span>\n                                        </div>\n                                    </el-col>\n                           </el-row>  \n                    </div>\n                    <div v-show=\"active==1\" class=\"register-item\">\n                           <el-row :gutter=\"20\">\n                                <el-col :span=\"6\">\n                                    <div class=\"grid-content bg-purple\">\n                                         <span class=\"item item1\">设置密码</span>\n                                    </div>\n                                    </el-col>\n                                <el-col :span=\"8\">\n                                    <div class=\"grid-content bg-purple\">\n                                        <el-input\n                                            placeholder=\"请输入密码\"\n                                            v-model=\"upwd\" type=\"password\"  @focus=\"upwdFocus\" @blur=\"upwdBlur\"\n                                            clearable>\n                                        </el-input>\n                                    </div>\n                                </el-col>\n                                <el-col :span=\"10\">\n                                    <div class=\"grid-content bg-purple\">\n                                         <span class=\"item item2\" :class=\"item3Color\">\n                                             <i :class=\"item3Icon\"></i>\n                                               {{msg3}}\n                                         </span>\n                                    </div>\n                                </el-col>\n                           </el-row> \n                             <el-row :gutter=\"20\">\n                                <el-col :span=\"6\">\n                                    <div class=\"grid-content bg-purple\">\n                                         <span class=\"item item1\">确认密码</span>\n                                    </div>\n                                    </el-col>\n                                <el-col :span=\"8\">\n                                    <div class=\"grid-content bg-purple\">\n                                        <el-input\n                                            placeholder=\"请确认密码\"\n                                            v-model=\"upwd1\" type=\"password\"  @focus=\"upwd1Focus\" @blur=\"upwd1Blur\"\n                                            clearable>\n                                        </el-input>\n                                    </div>\n                                </el-col>\n                                <el-col :span=\"10\">\n                                    <div class=\"grid-content bg-purple\">\n                                         <span class=\"item item2\" :class=\"item4Color\">\n                                             <i :class=\"item4Icon\"></i>\n                                               {{msg4}}\n                                         </span>\n                                    </div>\n                                </el-col>\n                           </el-row>  \n                    </div>\n                    <div v-show=\"active==2\" class=\"register-item\">\n                             <el-row :gutter=\"20\">\n                                <el-col :span=\"6\">\n                                    <div class=\"grid-content bg-purple\">\n                                         <button class=\"item item3\" @click=\"getTest\">点击获取验证码</button>\n                                    </div>\n                                    </el-col>\n                                <el-col :span=\"4\">\n                                    <div class=\"grid-content bg-purple\">\n                                        <el-input\n                                            placeholder=\"请输入验证码\"\n                                            v-model=\"test\" type=\"text\"  @blur=\"testBlur\"\n                                            clearable>\n                                        </el-input>\n                                    </div>\n                                </el-col>\n                                 <el-col :span=\"4\">\n                                    <div class=\"grid-content bg-purple\">\n                                        <el-input\n                                            v-model=\"test1\" type=\"text\" \n                                            clearable>\n                                        </el-input>\n                                    </div>\n                                </el-col>\n                                <el-col :span=\"10\">\n                                    <div class=\"grid-content bg-purple\">\n                                         <span class=\"item item2\" :class=\"item5Color\">\n                                             <i :class=\"item5Icon\"></i>\n                                               {{msg5}}\n                                         </span>\n                                    </div>\n                                </el-col>\n                           </el-row>    \n                    </div>\n                    <div v-show=\"active==3\" class=\"register-item\">\n                        <div class=\"register-success\">\n                            <p class=\"success  success1\">{{msg}}</P>\n                            <p class=\"success  success2\">\n                                <span class=\"success-time\">{{time}}</span>\n                                {{msg6}}\n                            </P>\n                        </div> \n                    </div>\n               </div>\n               <button @click=\"next\" class=\"btn\" v-show=\"active<=2\">下一步</button>\n        </div>\n    </div>\n</template>\n\n<script>\n  export default {\n    data() {\n      return {\n        active:0,\n        uname:null,\n        phone:null,\n        upwd:null,\n        upwd1:null,\n        test:null,\n        test1:null,\n        msg:\"\",\n        msg1:\"\",\n        msg2:\"\",\n        msg3:\"\",\n 